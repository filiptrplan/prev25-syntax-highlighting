{
  "name": "prev25-language",
  "displayName": "PREV'25 Language",
  "description": "Syntax highlighting for the PREV'25 language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "prev25",
        "aliases": [
          "PREV'25",
          "prev25"
        ],
        "extensions": [
          ".prev",
          ".prev25"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "prev25",
        "scopeName": "source.prev25",
        "path": "./prev25.tmLanguage.json"
      }
    ]
  }
}
